                <Pane id="titlePane" prefHeight="95.0" prefWidth="677.0" styleClass="title">
                    <children>
                        <Label id="titleText" prefHeight="57.0" prefWidth="641.0" styleClass="titleText" text="BRAIN BLAST QUESTIONS">
                            <padding>
                                <Insets bottom="20.0" left="30.0" top="20.0" />
                            </padding>
                        </Label>
      
                           <Button id="editBttn" fx:id="editButton" layoutX="580.0" layoutY="25.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="88.0" styleClass="titleOption" text="Edit">
                        <graphic>
                            <ImageView id="edit" fitHeight="23.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../images/edit_op.png" />
                                </image>
                            </ImageView>
                        </graphic>
                     </Button>
                        <Button id="saveBttn" fx:id="saveButton" layoutX="576.0" layoutY="68.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" styleClass="titleOption" text="Save">
