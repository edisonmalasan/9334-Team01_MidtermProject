                     <Label id="titleSubheadText" layoutX="14.0" layoutY="8.0" styleClass="titleSubheadText" text="TOTAL QUESTIONS:">
                        <graphic>
                            <ImageView fitHeight="31.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../images/list_icon.png" />
                                </image>
                            </ImageView>
                        </graphic></Label>
                     <Label id="titleSubheadText1" fx:id="totalQuestionNumberLabel" layoutX="237.0" layoutY="8.0" styleClass="titleSubheadText" text="00" />
