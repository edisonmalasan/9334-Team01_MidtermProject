                        <TableColumn id="choices" prefWidth="342.0" text="CHOICES">
                            <columns>
                                <TableColumn id="A" editable="true" prefWidth="85.0" text="A" />
                                <TableColumn id="B" editable="true" prefWidth="85.0" text="B" />
                           <TableColumn id="C" prefWidth="85.0" text="C" />
                                <TableColumn id="D" prefWidth="85.0" text="D" />
                            </columns>
                        </TableColumn>

                        <TableColumn id="endlessStats" editable="true" prefWidth="64.0" text="ANSWER" />
